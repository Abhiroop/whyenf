    (
      (ONCE (read(t1,x) OR write(t1,x)))
      AND
      (ONCE write(t2,x))
    )
    IMPLIES
    EXISTS l.
    (
      (NOT ONCE NOT ((read(t1,x) OR write(t1,x))
          IMPLIES (NOT rel(t1,l) SINCE acq(t1,l))))
      AND
      (NOT ONCE NOT ((read(t2,x) OR write(t2,x))
          IMPLIES (NOT rel(t2,l) SINCE acq(t2,l))))
    )